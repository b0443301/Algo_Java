package insertion_sort;

public class main {
	public static void main(String[] argv) {
		int temp = 0;
		int[] arr = { 8, 9, 7, 10, 6, 5, 4, 3, 2, 1 };
		for (int i = 0; i < 10; i++) {
			temp = arr[i];
			for (int j = 10;j > 0; j--) {
				if ( temp < arr[j - 1]) {
					arr[j] = arr[j - 1];
					arr[j] = temp;
				}
			}
		}
		for (int j = 0; j < 10; j++) {
			System.out.println(arr[j]);
		}
	}
}
