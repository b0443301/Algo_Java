package insertion_sort;

public class main {
	public static void main(String[] argv) {
		int temp = 0, i = 10, j = i;
		int[] arr = { 8, 9, 7, 10, 6, 5, 4, 3, 2, 1 };
		for (i = 0; i < 10; i++) {
			temp = arr[i];
			
			while ((arr[j - 1] >temp) && (j > 0)) {
				arr[j] = arr[j - 1];
				j--;
				temp= arr[j] ;
			}
			

			// for ( j = i; j > 0 && temp > arr[j - 1]; j--) {
			// arr[j] =arr[j - 1];
			// arr[j] = temp;
			// }
			// }

			
		}
		for (j = 0; j  < 10; j ++) {
			System.out.println(arr[j]);
		}
	}
}
