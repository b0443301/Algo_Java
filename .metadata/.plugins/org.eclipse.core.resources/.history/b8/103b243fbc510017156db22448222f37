package insertion_sort;

public class main {
	public static void main(String[] argv) {
		int temp = 0;
		int[] arr = { 8, 4, 12, 6, 9, 3, 7, 10, 2, 1 };
		for (int i = 1; i < 10; i++) {
			temp = arr[i];
			for (int j = 10; j > 0 && temp < arr[j - 1]; j--) {
				arr[j] = arr[j - 1];
				arr[j] = temp;
			}
		}

	}
}
